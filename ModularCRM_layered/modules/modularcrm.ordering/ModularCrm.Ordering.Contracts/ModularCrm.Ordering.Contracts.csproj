  </ItemGroup>

  <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="8.0.4" />
  </ItemGroup>

  <ItemGroup>
      <EmbeddedResource Include="Localization\Ordering\*.json" />
      <Content Remove="Localization\Ordering\*.json" />
  </ItemGroup>

</Project>
